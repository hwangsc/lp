/* common layout */
#bottom1{position:fixed;left:0;bottom:0;z-index:200;width:100%;height:90px;padding-top:30px;background:url(/WebContent/images/common/bg_bottom_shadow.png) repeat-x;background-size:1px 30px}
#bottom1.hidden{-webkit-transform:translateY(100px);transform:translateY(100px);-webkit-transition:-webkit-transform 0.5s, visibility 0s 0.5s;transition:transform 0.5s, visibility 0s 0.5s}
#bottom1 .bg_bx{overflow:hidden;width:100%;height:60px;border-top:1px solid #f2f2f2;background:#fff}
#bottom1 .bg_bx .l_btn{overflow:hidden;float:left;height:60px}
#bottom1 .bg_bx .r_btn{overflow:hidden;float:right;height:60px}
#bottom1 .bg_bx button{overflow:hidden;float:left;display:block;height:60px;padding:0 20px;vertical-align:middle;font-size:1em;font-weight:normal;color:#000;text-align:center;-webkit-flex;display:flex;-webkit-align-items:center;align-items:center}
#bottom1 .bg_bx button.active{background:#f2f2f2}
#bottom1 .bg_bx button span{opacity:1 !important}
#bottom1 .bg_bx button:disabled span{color:#999}
#bottom1 .bg_bx .line{float:left;display:inline-block;width:1px;height:100%;background:url(/WebContent/images/commerce/common/btm_line.png) 50% 50% no-repeat;background-size:1px 16px;text-indent:-9999px}/* 140421 수정 */
#bottom1 .bg_bx .on.active{background:#f2f2f2}
#bottom1 .bg_bx .on span{opacity:1 !important}
#bottom1 .bg_bx .btn_boxing{padding-left:53px;background:url(/WebContent/images/commerce/common/ico_boxing_d.png) 12px 50% no-repeat;background-size:34px 34px}
#bottom1 .bg_bx .btn_boxing span{display:none}
.shop+#bottom1 .bg_bx .on.btn_boxing{background:url(/WebContent/images/commerce/common/ico_boxing_shop.png) 12px 50% no-repeat;background-size:34px 34px}
.movie+#bottom1 .bg_bx .on.btn_boxing{background:url(/WebContent/images/commerce/common/ico_boxing_movie.png) 12px 50% no-repeat;background-size:34px 34px}
.coupon+#bottom1 .bg_bx .on.btn_boxing{background:url(/WebContent/images/commerce/common/ico_boxing_coupon.png) 12px 50% no-repeat;background-size:34px 34px}
.giftcard+#bottom1 .bg_bx .on.btn_boxing{background:url(/WebContent/images/commerce/common/ico_boxing_giftcard.png) 12px 50% no-repeat;background-size:34px 34px}
.webtoon+#bottom1 .bg_bx .on.btn_boxing{background:url(/WebContent/images/commerce/common/ico_boxing_webtoon.png) 12px 50% no-repeat;background-size:34px 34px}
#bottom1 .bg_bx .btn_boxing .count{font-family:roboto;font-size:0.875em;font-weight:normal;font-style:italic;color:#000}
#bottom1 .bg_bx .btn_prev_view{padding-left:38px;background:url(/WebContent/images/commerce/common/btn_prev_view.png) 0 50% no-repeat;background-size:28px 28px}
#bottom1 .bg_bx .btn_prev_view span{font-size:0.875em}
#bottom1 .bg_bx .btn_next_view{padding-right:38px;background:url(/WebContent/images/commerce/common/btn_next_view.png) 100% 50% no-repeat;background-size:28px 28px}
#bottom1 .bg_bx .btn_next_view span{font-size:0.875em}
#bottom1 .on-sale { position:fixed; left:0; right:0; bottom:60px; padding:10px 20px; font-size:0.875em; line-height:1.2; color:#ffffff; text-align:right; background-color:#8200c8; box-sizing:border-box; }
#bottom1 .on-sale:before { content:""; position:absolute; right:40px; bottom:-6px; width:0; height:0; vertical-align:middle; border:1px solid transparent; border-top-color:#8200c8; border-width:6px 6px 0; }

